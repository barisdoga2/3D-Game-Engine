package dev.engine.textures;

public class SpecularTexture {
	
	private String name;
	private int textureID;
	
	public NormalTexture(String name, int textureID) {
		this.name = name;
		this.textureID = textureID;
	}

	public String getName() {
		return name;
	}

	public int getTextureID() {
		return textureID;
	}
	
}
